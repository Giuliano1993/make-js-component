{
  "name": "make-js-component",
  "version": "0.1.5",
  "description": "Easily create your js framework component in one command",
  "repository": "https://github.com/Giuliano1993/make-js-component",
  "bin": "./cmd/make-js-component.mjs",
  "scripts": {
    "dev": "npx tsc -w",
    "test": "echo \"Error: no  test specified\" && exit 1",
    "format": "biome format ./src --write",
    "lint": "biome lint ./src"
  },
  "keywords": [
    "node",
    "js",
    "framework",
    "vue",
    "react",
    "qwik",
    "command",
    "npx"
  ],
  "author": "Giuliano Gostinfini (Ghostylab)",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.4.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.9.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12"
  }
}
